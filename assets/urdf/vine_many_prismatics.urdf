            </geometry>
            <origin xyz="0 0 0" rpy="0 1.57 0"/>
            <material name="red"/>
        </visual>
    </link>

    <!-- JOINTS -->
    <!-- Alternate between 1 revolute joint (link to virtual link) and 3 prismatic joints -->
    <!-- Segment lengths usually at most 1 meters, so use 0.5m (prismatic upper limit)-->
    <!-- Joint angles usually at most [-30, 30] degrees => [-0.5236, 0.5236] radians (revolute limits)-->
    <!-- Max growth speed is about 0.1m/s (prismatic joint velocity limit) -->
    <!-- Other efforts and velocities currently untuned -->
    <joint name="revolute_joint_0" type="revolute">
        <!-- Maybe should be rotation/continuous -->
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="base_virtual_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <!-- <limit lower="-3.146" upper="3.146" effort="10000" velocity="3"/> -->
        <limit lower="-0.5236" upper="0.5236" effort="10000" velocity="3"/>
    </joint>
    <joint name="prismatic_joint_0_0" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_virtual_link"/>
        <child link="link_0_0"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>
    <joint name="prismatic_joint_0_1" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="link_0_0"/>
        <child link="link_0_1"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>
    <joint name="prismatic_joint_0_2" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="link_0_1"/>
        <child link="link_0_2"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>
    <joint name="revolute_joint_1" type="revolute">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="link_0_2"/>
        <child link="virtual_link_0"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-0.5236" upper="0.5236" effort="10000" velocity="3"/>
    </joint>
    <joint name="prismatic_joint_1_0" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="virtual_link_0"/>
        <child link="link_1_0"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>
    <joint name="prismatic_joint_1_1" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="link_1_0"/>
        <child link="link_1_1"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>
    <joint name="prismatic_joint_1_2" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="link_1_1"/>
        <child link="link_1_2"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>


    <joint name="revolute_joint_2" type="revolute">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="link_1_2"/>
        <child link="virtual_link_1"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-0.5236" upper="0.5236" effort="10000" velocity="3"/>
    </joint>
    <joint name="prismatic_joint_2_0" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="virtual_link_1"/>
        <child link="link_2_0"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>
    <joint name="prismatic_joint_2_1" type="prismatic">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="link_2_0"/>
        <child link="link_2_1"/>
        <axis xyz="1.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.1" effort="10000" velocity="1"/>
    </joint>
    <joint name="prismatic_joint_2_2" type="prismatic">
