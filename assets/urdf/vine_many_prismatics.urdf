    </link>

    <link name="virtual_link_1"/>



    <link name="link_2_0">
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
        </inertial>

        <collision>
            <geometry>
                <cylinder radius="0.025" length="0.1"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 1.57 0"/>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.025" length="0.1"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 1.57 0"/>
            <material name="red"/>
        </visual>
    </link>

    <link name="link_2_1">
        <inertial>
            <mass value="0.01"/>
            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
        </inertial>

