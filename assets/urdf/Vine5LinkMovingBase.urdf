    <link name="cart">
        <visual>
            <geometry>
                <box size="0.2 0.25 0.2" />
            </geometry>
            <material name="black" />
        </visual>
        <collision>
            <geometry>
                <box size="0.2 0.25 0.2" />
            </geometry>
        </collision>
        <inertia>
            <mass value="1" />
        </inertia>
    </link>

