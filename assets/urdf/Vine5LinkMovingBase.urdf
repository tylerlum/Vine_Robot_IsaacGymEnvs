    <!-- 1 slider link, 1 cart link, and 5 cylindrical links -->

    <!-- SLIDER -->
    <!-- Long box arbitrary dimensions -->
    <link name="slider">
        <visual>
            <geometry>
                <box size="0.03 8 0.03" />
            </geometry>
            <material name="iron" />
        </visual>
        <collision>
            <geometry>
                <box size="0.03 8 0.03" />
            </geometry>
        </collision>
    </link>

    <!-- CART -->
